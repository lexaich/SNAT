 {
  "manifest_version": 2,
  "name": "SNAT",
  "version": "0.0.1",
  
    "icons": {
    "48": "128.png"
  },
  "content_scripts": 
  [ 

    {
      // "all_frames": true,
      // "match_about_blank": true,
      "js": [ "jquery.js" ,"content.js"],
      "matches": ["*://*.twitter.com/*"],
      "run_at": "document_start"    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "background": {
      "persistent": false,
      "scripts": [ "background.js", "jquery.js" ]
   },

   "permissions": [ "tabs", "cookies", "activeTab", "storage", "management","*://*.twitter.com/*"],

      "browser_action": {
        "default_title": "SNAT",
        "default_popup":"popup.html"
    }
	
}